          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonOutput

def productPrices = [
    1: 599.99, 2: 1299.99, 3: 499.99, 4: 149.99, 5: 19.99,
    6: 49.99, 7: 24.99, 8: 29.99, 9: 14.99, 10: 39.99
]

def random = new Random()
def totalAmount = 0.00
def products = []

def productIds = (1..10).toList().shuffled(random)

for (int i = 0; i &lt; 4; i++) {
    def productId = productIds[i]
    def quantity = random.nextInt(5) + 1

    def price = productPrices[productId] ?: 0.00
    totalAmount += price * quantity

    products.add([
        &quot;productId&quot;: productId,
        &quot;quantity&quot;: quantity
    ])
}

vars.put(&quot;requestedAmount&quot;, String.format(Locale.ENGLISH, &quot;%.2f&quot;, totalAmount))
vars.put(&quot;productsJson&quot;, JsonOutput.toJson(products))
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
